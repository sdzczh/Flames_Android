<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:layout_width="match_parent"
    android:paddingLeft="15dp"
    android:paddingRight="15dp"
    android:layout_height="40dp">

    <TextView
        android:id="@+id/tv_myteam_phone"
        android:text="88***88"
        android:textSize="13.33sp"
        android:textColor="@color/color_e6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv_myteam_time"
        android:text="2019.11.05 11:00"
        android:textSize="14sp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/color_e6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv_myteam_profit"
        android:layout_alignParentRight="true"
        android:textSize="14sp"
        android:textColor="@color/txt_all8"
        android:text="1.00PGY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



</RelativeLayout>